<h1>Create a new Trail</h1>

<%= form_for :trail, url: trails_path do |f| %>
  <p>
    <%= f.label :trail_name %></br>
    <%= f.text_field :trail_name %> </br>
  </p>
  <p>
    <%= f.label :trail_info  %></br>
    <%= f.text_area :trail_info %></br>
  </p>
  <p>
    <%= f.submit %>
  </p>

<% end %>